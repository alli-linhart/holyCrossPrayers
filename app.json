{
  "expo": {
    "name": "Prayers for Your Day",
    "slug": "holyCrossPrayers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/prayerHands.png",
    "scheme": "holycrossprayers",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alli-linhart.holyCrossPrayers"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/prayerHands.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.allilinhart.holyCrossPrayers"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/csc-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/prayerHands.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
